{
    "name": "pacerit/bitbay-api-php",
    "description": "Implementation of BitBayPay REST API",
    "type": "library",
    "keywords": [
        "bitbaypay",
        "bitbayapi",
        "bitbay"
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "illuminate/support": "^7.0",
        "guzzlehttp/guzzle": "^6.5",
        "ramsey/uuid": "^3.9",
        "rakit/validation": "^1.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpoption/phpoption": "^1.7",
        "vlucas/phpdotenv": "^4.1"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Wiktor Pacer",
            "email": "kontakt@pacerit.pl",
            "role": "Developer",
            "homepage": "https://pacerit.pl"
        }
    ],
    "autoload": {
        "psr-4": {
            "PacerIT\\BitBayPayAPI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PacerIT\\BitBayPayAPI\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "minimum-stability": "stable"
}
